<h2>Show poll</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @poll.name %>
  </li>

  <li>
    <strong>Shortcut:</strong>
    <%= @poll.shortcut %>
  </li>

  <li>
    <strong>Creator:</strong>
    <%= @poll.creator_id %>
  </li>

</ul>

<%= link "Edit", to: poll_path(@conn, :edit, @poll) %>
<%= link "Back", to: poll_path(@conn, :index) %>
